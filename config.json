{
    "disable_auto_save": true,
    "log": {
        "level": "debug",
        "file": "./.logs/yambol.log"
    },
    "api": {
        "rest": {
            "enabled": true,
            "port": 21419
        },
        "grpc": {
            "port": 21421
        },
        "certificate": ".tls/yambol.crt",
        "key": ".tls/yambol.key"
    },
    "broker": {
        "default_min_length": 12,
        "default_max_length": 2000,
        "default_max_size_bytes": 10485760,
        "queues": {
            "default": { },
            "test": {
                "min_length": 100,
                "max_length": 10000
            }
        }
    }
}
